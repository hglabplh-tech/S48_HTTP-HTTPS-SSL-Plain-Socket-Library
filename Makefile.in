# OCI48 Makefile
# Documentation in files INSTALL and doc/install.txt

SHELL = /bin/sh

### Filled in by `configure' ###
srcdir = @srcdir@
VPATH = @srcdir@
CC = @CC@
DEFS = @DEFS@
LIBS = @LIBS@
CFLAGS =  -Wmissing-prototypes -pthread 
# for the time of development added hardcoded
DEBUG_FLAGS = -g
INSTALL = @INSTALL@
INSTALL_PROGRAM = @INSTALL_PROGRAM@
INSTALL_DATA = @INSTALL_DATA@
SYSDEP=@SYSDEP@
INSTALL_ADD=@INSTALL_ADD@
LDFLAGS = @LDFLAGS@
LIBOBJS = @LIBOBJS@
S48SSLLIB = libsslhttp48.so
RM	= rm -f

DYNAMIC_EXTERNALS_LDFLAGS=@DYNAMIC_EXTERNALS_LDFLAGS@
DYNAMIC_EXTERNALS_CFLAGS=@DYNAMIC_EXTERNALS_CFLAGS@
DYNAMIC_EXTERNALS_LD=@DYNAMIC_EXTERNALS_LD@
DYNAMIC_EXTERNALS_CC=@DYNAMIC_EXTERNALS_CC@

ext_incflags=@ext_incflags@
ora = @oralib@
AR	= @AR@
RANLIB	= @RANLIB@

prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@
libdir = @libdir@
incdir = @includedir@
manext = 1
mandir = @mandir@/man$(manext)
lib_dirs_list = @lib_dirs_list@
host = @host@
### End of `configure' section###

.c.o: 
	echo compile
	$(DYNAMIC_EXTERNALS_CC) -c $(DEFS) -I./c $(ext_incflags) $(DYNAMIC_EXTERNALS_CFLAGS) $(DEBUG_FLAGS) $(CPPFLAGS) $(CFLAGS) -o $@ $<
# Targets:

SSL48OBJS= \ 
	$(srcdir)/c/ssl48.o \
	$(srcdir)/c/sslutils.o

all: $(S48SSLLIB)
configure: configure.in
	autoconf
clean:
	rm $(S48SSLLIB) $(SSL48OBJS)

$(OCI48OBJS): $(srcdir)/c/ssl48.h $(srcdir)/c/sslutils.h


$(S48OCILIB): $(OCI48OBJS)
	$(DYNAMIC_EXTERNALS_LD) $(DYNAMIC_EXTERNALS_LDFLAGS) $(LDFLAGS) $(SSL48OBJS) $(LIBS) -o $(S48SSLLIB)

